			"item": [
				{
					"name": "/characters",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"function converted_day_start() {\r",
									"    let data = new Date();\r",
									"    let dia  = data.getDate().toString();\r",
									"    let diaF = (dia.length == 1) ? '0'+dia : dia;\r",
									"    let mes  = (data.getMonth()+1).toString();\r",
									"    let mesF = (mes.length == 1) ? '0'+mes : mes;\r",
									"    let anoF = data.getFullYear();\r",
									"    return anoF + \"-\" + mesF + \"-\" + diaF;\r",
									"}\r",
									"\r",
									"const data_inicio = converted_day_start();\r",
									"\r",
									"function convert_hour_start() {\r",
									"    let hora = new Date();\r",
									"    let horas = hora.getHours();\r",
									"    let horasF = (hora.length == 1) ? '0'+horas : horas;\r",
									"    let minutos = hora.getMinutes();\r",
									"    let minutosF = (minutos.length == 1) ? '0'+minutos : minutos;\r",
									"    let segundos = hora.getSeconds();\r",
									"    let segundosF = (segundos.length == 1) ? '0+segundos' : segundos;\r",
									"    return horasF + \":\" + minutosF + \":\" + segundosF;\r",
									"}\r",
									"\r",
									"const hora_inicio = convert_hour_start();\r",
									"\r",
									"let d = new Date(data_inicio + \" \" + hora_inicio);\r",
									"let d1 = d.getTime().toString();\r",
									"const timestamp = d1.replace(/.{0,3}$/, '');\r",
									"console.log(timestamp);\r",
									"pm.globals.set('timestamp', timestamp);\r",
									"\r",
									"const privateKey = '19318eff518a911aaf625a55483451195754d082';\r",
									"const publicKey = '51135bb3bca517200fc71edf3ca27899';\r",
									"console.log(publicKey);\r",
									"\r",
									"let concatenatedString = timestamp + privateKey + publicKey;\r",
									"\r",
									"const md5Hash = CryptoJS.MD5(concatenatedString);\r",
									"pm.globals.set('publicKey', publicKey);\r",
									"pm.globals.set('md5Hash', md5Hash.toString());\r",
									"console.log(md5Hash.toString());\r",
									""
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", () => {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"pm.test(\"Messagem is OK\", () => {\r",
									"    pm.response.to.be.ok;\r",
									"});\r",
									"\r",
									"pm.test(\"response must be valid and have a body\", () => {\r",
									"     pm.response.to.be.withBody;\r",
									"     pm.response.to.be.json;\r",
									"});\r",
									"\r",
									"pm.test(\"Validate non-empty response\", () => {\r",
									"    pm.response.to.not.be.empty;\r",
									"});\r",
									"\r",
									"const results = pm.response.json().data.results;\r",
									"const randomId = _.sample(results).id;\r",
									"console.log('ID aleatório:', randomId);\r",
									"pm.environment.set('characterId', randomId);\r",
									""
								],
								"type": "text/javascript"
							}
						}
